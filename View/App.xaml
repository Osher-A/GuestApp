<Application x:Class="GuestApp.View.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:converter="clr-namespace:GuestApp.Utility.Converters"
             xmlns:local="clr-namespace:GuestApp.View">
    <Application.Resources>
        <ResourceDictionary>
            <local:ViewModelLocator x:Key="viewModelEntry" />
            <converter:GuestTitleConverter x:Key="TitleConverter" />
            <converter:VisibilityConverter x:Key="VisibilityConverter" />
            <converter:GuestGroupConverter x:Key="GroupConverter" />
            <converter:DaysRemainingConverter x:Key="EventDaysRemainingConverter" />
        </ResourceDictionary>
    </Application.Resources>
</Application>